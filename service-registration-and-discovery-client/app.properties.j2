spring.application.name={{ app_name }}
info.app.name={{ app_name }} on {{ app_port }} port
info.app.description= MS Client App
info.app.version= {{ app_version }}
server.port={{ app_port }}
logging.level.org.springframework=DEBUG
eureka.client.service-url.defaultZone=http://{{ springboot_backend_name }}:8761/eureka

management.endpoints.web.exposure.include = health, info, shutdown, beans 
management.info.env.enabled=true
management.info.build.enabled=true
management.info.git.enabled=true
management.info.java.enabled=true
management.info.os.enabled=true